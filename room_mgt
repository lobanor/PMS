        regFace.setVisible(true); 
        
        Suite_Admin.Btn1_Up.setBackground(Color.red); 
        Suite_Admin.Guest_Lbl.setText(" ");
       
       dispose(); 
    }//GEN-LAST:event_Make_UnavailableMouseClicked

