//String EndD = (String) End_Date_Txt.getText ( );
//String RoomNote = (String) Reason_Cmb.getSelectedItem( );

DefaultTableModel model=( DefaultTableModel)  Unavailable_Tbl.getModel();
model.addRow(new Object[] { Start_Date_Txt.getText  (), End_Date_Txt.getText (), (String) Reason_Cmb.getSelectedItem ()   } ) ;

        
